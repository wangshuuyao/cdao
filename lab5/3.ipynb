   "id": "c97e368b8a13bfbd"
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "outputs": [],
   "source": [
    "def insertion_sort(arr):  \n",
    "    n = len(arr)  \n",
    "    for i in range(1, n):  \n",
    "        key = arr[i]  \n",
    "        j = i - 1  \n",
    "        while j >= 0 and arr[j] > key:  \n",
    "            arr[j + 1] = arr[j]  \n",
    "            j -= 1  \n",
    "        arr[j + 1] = key  \n",
    "    return arr"
   ],
